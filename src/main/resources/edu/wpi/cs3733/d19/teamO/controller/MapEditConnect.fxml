<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="233.0" prefWidth="546.0" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="edu.wpi.cs3733.d19.teamO.controller.MapEditConnectController">
  <children>
    <HBox layoutX="87.0" layoutY="41.0" spacing="15.0">
      <children>
        <VBox layoutX="120.0" layoutY="137.0" spacing="20.0">
          <children>
            <Label text="EdgeID:">
              <font>
                <Font size="22.0" />
              </font>
            </Label>
            <Label layoutX="82.0" layoutY="193.0" text="NodeID_1:">
              <font>
                <Font size="22.0" />
              </font>
            </Label>
            <Label text="NodeID_2:">
              <font>
                <Font size="22.0" />
              </font>
            </Label>
          </children>
        </VBox>
        <VBox layoutX="227.0" layoutY="148.0" spacing="10.0">
          <children>
            <JFXTextField fx:id="edgeID" onKeyReleased="#validateButton">
              <font>
                <Font size="18.0" />
              </font>
            </JFXTextField>
            <JFXTextField fx:id="connectNodeID1" layoutX="277.0" layoutY="148.0" onKeyReleased="#validateButton">
              <font>
                <Font size="18.0" />
              </font>
            </JFXTextField>
            <JFXTextField fx:id="connectNodeID2" onKeyReleased="#validateButton">
              <font>
                <Font size="18.0" />
              </font>
            </JFXTextField>
          </children>
        </VBox>
      </children>
    </HBox>
    <JFXButton fx:id="connectButton" disable="true" layoutX="394.0" layoutY="190.0" onAction="#connectNodeAction" text="Connect">
      <font>
        <Font size="14.0" />
      </font>
    </JFXButton>
    <JFXButton fx:id="cancel" layoutX="470.0" layoutY="190.0" onAction="#cancelButtonAction" text="Cancel">
      <font>
        <Font size="14.0" />
      </font>
    </JFXButton>
    <Label fx:id="status" layoutX="206.0" layoutY="183.0" textFill="RED">
      <font>
        <Font size="14.0" />
      </font>
    </Label>
  </children>
</AnchorPane>
