            <TableColumn fx:id="locationTableCol" editable="false" maxWidth="300.0" minWidth="50.0" prefWidth="0.0" text="LocationNodeID" />
            <TableColumn fx:id="categoryTableCol" editable="false" maxWidth="300.0" minWidth="50.0" prefWidth="0.0" text="SanitationType" />
            <TableColumn fx:id="descriptionCol" editable="false" maxWidth="300.0" minWidth="50.0" prefWidth="0.0" text="Description" />
          </columns>
        </TableView>
      </children>
    </AnchorPane>
  </center>
