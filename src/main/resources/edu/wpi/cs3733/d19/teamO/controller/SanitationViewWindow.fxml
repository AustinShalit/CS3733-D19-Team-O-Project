<BorderPane stylesheets="@v2/style/KioskStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.d19.teamO.controller.SanitationViewWindowController">
  <center>
    <AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@v2/style/KioskStyle.css">
