          <font>
            <Font size="32.0" />
          </font>
        </Label>
        <TableView fx:id="requestsTableView" layoutX="50.0" layoutY="62.0" prefHeight="338.0" prefWidth="250.0" stylesheets="@v2/style/KioskStyle.css" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="57.0">
