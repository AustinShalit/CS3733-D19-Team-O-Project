        <GridPane.margin>
          <Insets bottom="1.0" left="1.0" right="1.0" top="1.0"/>
        </GridPane.margin>
      </JFXButton>
      <JFXButton fx:id="securityButton" onAction="#securityOnAction" prefHeight="325.0"
                 prefWidth="325.0" style="-fx-background-color: grey;" text="Security"
                 GridPane.columnIndex="2" GridPane.rowIndex="1">
        <GridPane.margin>
          <Insets bottom="1.0" left="1.0" right="1.0" top="1.0"/>
        </GridPane.margin>
      </JFXButton>
    </GridPane>
  </HBox>
  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
    <Pane prefHeight="200.0" prefWidth="200.0"/>
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Scrolling announcements"
