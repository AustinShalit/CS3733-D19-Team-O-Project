<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="441.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="edu.wpi.cs3733.d19.teamO.controller.LoginWindowController">
    <children>
        <Label layoutX="96.0" layoutY="65.0" text="Login">
            <font>
                <Font size="39.0"/>
            </font>
        </Label>
        <VBox layoutX="98.0" layoutY="154.0" spacing="12.0">
            <children>
                <JFXTextField fx:id="userName" promptText="Username">
                    <font>
                        <Font size="23.0"/>
                    </font>
                </JFXTextField>
                <JFXPasswordField fx:id="passWord" prefHeight="36.0" prefWidth="258.0"
                                  promptText="Password">
                    <font>
                        <Font size="23.0"/>
                    </font>
                </JFXPasswordField>
            </children>
        </VBox>
        <JFXButton fx:id="loginButton" layoutX="287.0" layoutY="310.0" onAction="#loginButtonAction"
                   text="Login">
            <font>
                <Font size="18.0"/>
            </font>
        </JFXButton>
        <Label fx:id="loginFail" layoutX="72.0" layoutY="277.0" prefHeight="39.0" prefWidth="338.0">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>